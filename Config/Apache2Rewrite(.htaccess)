RewriteEngine On

RewriteRule ^Ajax$ /Lander/Page/AjaxLander.php?module_name=Index&page_name=Index [L]
RewriteRule ^/Ajax/\w+/\w+$ /Lander/Page/AjaxLander.php?module_name=$1&page_name=$2 [L]
RewriteRule ^/Ajax/\w+/\w+/\w+$ Lander/Page/AjaxLander.php?module_name=$1&page_name=$2&key=$3 [L]
RewriteRule ^$ /Lander/Page/IndexLander.php?module_name=Index&page_name=Index [L]
RewriteRule ^/\w+/\w+$ /Lander/Page/IndexLander.php?module_name=$1&page_name=$2 [L]
RewriteRule ^/\w+/\w+/\w+$ /Lander/Page/IndexLander.php?module_name=$1&page_name=$2&key=$3 [L]
